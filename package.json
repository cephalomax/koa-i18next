{
  "name": "@cephalomax/koa-i18next",
  "version": "1.1.2",
  "description": "koa middleware for i18next",
  "main": "./lib/index.js",
  "scripts": {
    "test": "ava | tap-spec",
    "watch:test": "ava --watch | tap-spec",
    "build": "NODE_ENV=production rollup -c",
    "prepublish": "npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "lib/index.js"
  ],
  "keywords": [
    "i18n",
    "i18next",
    "koa",
    "middleware"
  ],
  "author": "sunfuze <sunfuze.1989@gmail.com>",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "tap": "tap-spec",
    "timeout": "5s"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sunfuze/koa-i18next.git"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babelrc-rollup": "^1.1.0",
    "i18next": "^10.6.0",
    "i18next-sync-fs-backend": "^1.0.0",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.1",
    "rollup": "^0.59.1",
    "rollup-plugin-babel": "^2.7.1",
    "supertest": "^1.2.0",
    "tap-spec": "^4.1.1"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "koa-convert": "^1.2.0"
  }
}
